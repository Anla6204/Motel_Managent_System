<Window x:Class="AnLaNPWPF.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:AnLaNPWPF"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <StackPanel VerticalAlignment="Center" HorizontalAlignment="Center">
            <Button Name="TestConfigButton" Content="Test Configuration" Width="200" Height="40" Margin="10" Click="TestConfigButton_Click"/>
            <Button Name="LoginButton" Content="Go to Login" Width="200" Height="40" Margin="10" Click="LoginButton_Click"/>
        </StackPanel>
    </Grid>
</Window>
